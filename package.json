{
  "name": "transaction-broadcaster",
  "version": "1.0.0",
  "engines": {
    "node": "16"
  },
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rimraf dist",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"./src/**/*.ts\" --fix",
    "pre:push": "npm run format && npm run lint & npm run build",
    "start:dev": "cross-env NODE_OPTIONS='--stack-trace-limit=50' ts-node -r dotenv/config ./src/index.ts",
    "start": "node --max-old-space-size=8192 -r dotenv/config ./dist/index.js",
    "start:fb:example": "ts-node -r dotenv/config ./src/flashbots-broadcaster/example.ts",
    "update:lib": "npm install @infinityxyz/lib@latest",
    "deploy:dev": "npm run build && gcloud app deploy --appyaml=./app.yaml --project=nftc-dev",
    "deploy:prod": "npm run build && gcloud app deploy --appyaml=./app.yaml --project=nftc-infinity"
  },
  "devDependencies": {
    "@types/node": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "@flashbots/ethers-provider-bundle": "^0.4.3",
    "@infinityxyz/lib": "^1.75.0",
    "@uniswap/sdk-core": "^3.0.1",
    "@uniswap/v3-sdk": "^3.8.2",
    "dotenv": "^16.0.1",
    "ethers": "^5.6.5",
    "firebase-admin": "^10.2.0",
    "phin": "^3.6.1"
  }
}
